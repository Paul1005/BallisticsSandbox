<UserControl x:Class="BallisticsSandbox.Output"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BallisticsSandbox"
             mc:Ignorable="d" 
             Height="700"
             Width="525">
    <Grid>
        <TextBox x:Name="KineticEnergy" HorizontalAlignment="Left" Height="23" Margin="100,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Recoil" HorizontalAlignment="Left" Height="23" Margin="57,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Kinetic Energy" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Recoil" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
